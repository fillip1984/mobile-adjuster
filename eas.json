{
  "cli": {
    "appVersionSource": "local",
    "version": ">= 7.6.0"
  },
  "build": {
    "development": {
      "environment": "development",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "environment": "preview",
      "distribution": "internal"
    },
    "production": {
      "environment": "production",
      "autoIncrement": true,
      "distribution": "internal"
    }
  }
}
